{
	"build": {
		"beforeDevCommand": "npm run dev:ui",
		"beforeBuildCommand": "npm run build:ui",
		"devPath": "http://localhost:1420",
		"distDir": "../dist",
		"withGlobalTauri": false
	},
	"package": {
		"productName": "brans-braid",
		"version": "0.0.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"globalShortcut": {
				"all": true
			}
		},
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "brans-braid",
			"icon": [
				"icons/icon.ico",
				"icons/icon.png"
			]
		},
		"security": {
			"csp": null
		},
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "brans-braid",
				"width": 1080,
				"height": 600
			}
		]
	}
}